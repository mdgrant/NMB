	journal = {Anesth Analg},
	keywords = {Androstanols/*pharmacology Humans Monitoring, Physiologic/*instrumentation Neuromuscular Junction/*drug effects/physiology Neuromuscular Nondepolarizing Agents/*pharmacology Prospective Studies Rocuronium},
