	isbn = {0003-2999},
	journal = {Anesth Analg},
	keywords = {Adult Aged Analysis of Variance Anesthesia Recovery Period *Anesthesia, General *Electromyography/instrumentation Equipment Design Female Humans Male Middle Aged Neuromuscular Blockade/*methods Neuromuscular Junction/*drug effects/physiopathology Neuromuscular Monitoring/instrumentation/*methods Neuromuscular Nondepolarizing Agents/*therapeutic use Predictive Value of Tests Reproducibility of Results},
	la = {eng},
	number = {2},
