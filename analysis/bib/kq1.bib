	keywords = {Adult Aged Anesthesia, General Female Hand/*physiology Humans Male Middle Aged Monitoring, Intraoperative/*instrumentation Motor Activity/*drug effects Movement *Neuromuscular Blockade Prospective Studies Skin Temperature/physiology},
	la = {eng},
	number = {1},
	pages = {33-8},
	st = {TOF-tube, a new protection for acceleromyography, compared with the TOF-Guard/TOF-Watch arm board},
