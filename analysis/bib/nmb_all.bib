	journal = {Surg Laparosc, Endosc Percutan Tech},
	number = {5},
	pages = {420-3},
	rn = {Adult Bariatric Surgery* Female Humans Laparoscopy* Male Neostigmine / therapeutic use Obesity, Morbid / surgery* Pain Measurement Pain, Postoperative / prevention \& control* Random Allocation Sugammadex gamma-Cyclodextrins / therapeutic use* Randomized Controlled Trial},
	st = {Sugammadex reduces postoperative pain after laparoscopic bariatric surgery: a randomized trial},
	title = {Sugammadex reduces postoperative pain after laparoscopic bariatric surgery: a randomized trial},
	volume = {24},
