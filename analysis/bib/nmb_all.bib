	an = {26126940},
	annote = {1432-055x Rahe-Meyer, N Berger, C Wittmann, M Solomon, C Abels, E A M Rietbergen, H Reuter, D A Clinical Trial, Phase III Journal Article Multicenter Study Randomized Controlled Trial Research Support, Non-U.S. Gov't Germany Anaesthesist. 2015 Jul;64(7):506-12. doi: 10.1007/s00101-015-0048-0. Epub 2015 Jul 1.},
