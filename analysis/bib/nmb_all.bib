	journal = {Anaesth Intensive Care},
	keywords = {Anesthesia Department, Hospital Dose-Response Relationship, Drug Electric Stimulation Female Humans Male Middle Aged Monitoring, Physiologic Muscle Relaxants, Central/*adverse effects Neuromuscular Blockade/*adverse effects New Zealand/epidemiology *Postoperative Care Postoperative Complications/*epidemiology/physiopathology *Recovery Room Respiration, Artificial Treatment Outcome},
	la = {eng},
