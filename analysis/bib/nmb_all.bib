	journal = {Journal of Clinical Monitoring and Computing},
	keywords = {adult alertness anesthesiology article controlled study drug combination drug therapy electronic medical record female human intraoperative monitoring male neuromuscular blocking peripheral nerve stimulator quantitative analysis retrospective study tracheal extubation neostigmine rocuronium vecuronium},
	la = {English},
