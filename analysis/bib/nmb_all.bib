	journal = {Anesthesiology},
	keywords = {Adolescent Adult *Androstanols Electric Stimulation Eyelids/innervation Facial Nerve/*physiology Female Humans Laryngeal Masks Laryngeal Muscles/*drug effects Male Middle Aged *Neuromuscular Blockade *Neuromuscular Nondepolarizing Agents Orbit Rocuronium},
	la = {eng},
