	journal = {Anesthesiology},
	keywords = {Adult Aged *Anesthesia Recovery Period Biomechanical Phenomena Confidence Intervals Electromyography/methods Female Humans Male Middle Aged Monitoring, Intraoperative/*methods Neuromuscular Blockade/adverse effects Paralysis/chemically induced/*diagnosis/physiopathology Predictive Value of Tests Probability Statistics, Nonparametric},
	la = {eng},
