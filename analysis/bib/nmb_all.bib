	la = {eng},
	number = {6},
	pages = {811-4},
	st = {Does repetition of post-tetanic count every 3 min during profound relaxation affect accelerographic recovery of atracurium blockade?},
	title = {Does repetition of post-tetanic count every 3 min during profound relaxation affect accelerographic recovery of atracurium blockade?},
