	author = {Plaud, B. and Debaene, B. and Donati, F.},
	date = {Jul},
	date-added = {2021-11-12 19:55:29 -0600},
	date-modified = {2021-11-12 19:55:42 -0600},
	doi = {10.1097/00000542-200107000-00019},
