	annote = {2005-7563 Cheong, Soon Ho Ki, Seunghee Lee, Jiyong Lee, Jeong Han Kim, Myoung-Hun Hur, Dongki Cho, Kwangrae Lim, Se Hun Lee, Kun Moo Kim, Young-Jae Lee, Wonjin Journal Article Korean J Anesthesiol. 2015 Dec;68(6):547-55. doi: 10.4097/kjae.2015.68.6.547. Epub 2015 Nov 25.},
