	an = {18385265},
	annote = {1471-6771 Flockton, E A Mastronardi, P Hunter, J M Gomar, C Mirakhur, R K Aguilera, L Giunta, F G Meistelman, C Prins, M E Clinical Trial, Phase III Journal Article Multicenter Study Randomized Controlled Trial Research Support, Non-U.S. Gov't England Br J Anaesth. 2008 May;100(5):622-30. doi: 10.1093/bja/aen037. Epub 2008 Apr 2.},
