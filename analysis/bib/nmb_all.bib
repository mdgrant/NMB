	author = {Karwacki, Z. and Niewiadomski, S. and Rzaska, M.},
	date-added = {2021-11-12 19:55:29 -0600},
	date-modified = {2021-11-12 19:55:42 -0600},
	doi = {10.5603/ait.2015.0042},
