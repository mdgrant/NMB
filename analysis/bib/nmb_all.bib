	journal = {Acta Anaesthesiologica Scandinavica},
	keywords = {adult article body temperature clinical article female human hypothermia male neuromuscular blocking},
	la = {English},
	number = {3},
	pages = {247-252},
	st = {Twitch tension and train-of-four ratio during prolonged neuromuscular monitoring at different peripheral temperatures},
	title = {Twitch tension and train-of-four ratio during prolonged neuromuscular monitoring at different peripheral temperatures},
	type = {Article},
	url = {https://www.embase.com/search/results?subaction=viewrecord&id=L21128452&from=export},
