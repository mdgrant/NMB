	journal = {Braz J Anesthesiol (English Edition)},
	number = {6},
	pages = {400-5},
	st = {Sugammadex versus neostigmine in pediatric patients: a prospective randomized study},
	title = {Sugammadex versus neostigmine in pediatric patients: a prospective randomized study},
	volume = {64},
