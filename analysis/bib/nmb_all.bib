	an = {9014620},
	annote = {Abdulatif, M Mowafi, H al-Ghamdi, A el-Sanabary, M Clinical Trial Comparative Study Journal Article Randomized Controlled Trial England Br J Anaesth. 1996 Dec;77(6):710-5. doi: 10.1093/bja/77.6.710.},
