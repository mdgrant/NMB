	keywords = {Adult Aged Aged, 80 and over Androstanols/*pharmacology *Anesthesia, General Female Humans Male Middle Aged *Neuromuscular Blockade Neuromuscular Nondepolarizing Agents/*pharmacology Rocuronium},
