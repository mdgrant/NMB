	annote = {de Boer, Hans D Driessen, Jacques J Marcus, Marco A E Kerkkamp, Hans Heeringa, Marten Klimek, Markus Clinical Trial, Phase II Journal Article Multicenter Study Randomized Controlled Trial Research Support, Non-U.S. Gov't United States Anesthesiology. 2007 Aug;107(2):239-44. doi: 10.1097/01.anes.0000270722.95764.37.},
	author = {e Boer, H. D. and Driessen, J. J. and Marcus, M. A. and Kerkkamp, H. and Heeringa, M. and Klimek, M.},
	date = {Aug},
