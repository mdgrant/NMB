	an = {34226418},
	annote = {1365-2346 Suganuma, Emiri Ishikawa, Teruhiko Kitamura, Yuji Hayashida, Taiichiro Matsumura, Tomoaki Fujie, Mai Nozaki-Taguchi, Natsuko Sato, Yasunori Isono, Shiroh Journal Article Randomized Controlled Trial England Eur J Anaesthesiol. 2021 Aug 1;38(8):856-864. doi: 10.1097/EJA.0000000000001464.},
