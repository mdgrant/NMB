	annote = {L{\o}vstad, R Z Thagaard, K S Berner, N S Raeder, J C Clinical Trial Journal Article Randomized Controlled Trial England Acta Anaesthesiol Scand. 2001 Apr;45(4):495-500. doi: 10.1034/j.1399-6576.2001.045004495.x.},
	author = {L{\o}vstad, R. Z. and Thagaard, K. S. and Berner, N. S. and Raeder, J. C.},
	date = {Apr},
	date-added = {2021-11-12 19:55:29 -0600},
