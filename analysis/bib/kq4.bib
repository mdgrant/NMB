	journal = {Anesthesiology},
	keywords = {Adolescent Adult Aged Androstanols/pharmacokinetics/*pharmacology Anesthetics, Inhalation/*pharmacology Dose-Response Relationship, Drug Female Humans Isoflurane/*pharmacology Liver Cirrhosis/*physiopathology Male Middle Aged Neuromuscular Nondepolarizing Agents/*pharmacology Rocuronium},
	la = {eng},
